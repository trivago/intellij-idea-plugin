<idea-plugin>
    <id>com.trivago.intellij.plugin</id>
    <name>trivago</name>

    <description>
        Internal IntelliJ plugin by trivago.
        Features:
        - Autogenerated classes to protobuf navigation
    </description>
    <vendor>trivago</vendor>

    <version>0.1.0</version>

    <idea-version since-build="193.0" until-build="193.*" />

    <extensions defaultExtensionNs="com.intellij">

        <psi.referenceContributor implementation="com.trivago.jetbrains.plugin.PsiProtoClassCommentReferenceContributor"/>

    </extensions>

</idea-plugin>